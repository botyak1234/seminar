#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
#include <cstring>
#include <fstream>
#include <vector>
using namespace std;




int fib(int n)
{
	if (n >= 3)
	{
		if (n == 3) return 1;
		else if (n == 5) return 1;
		return(fib(n - 3) + fib(n - 5));
	}
	else return 0;
}

int main()
{
	int n; cin >> n;
	cout << fib(n);
}
