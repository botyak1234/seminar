#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <vector>
#include <string>
using namespace std;



int main()
{
	int n; cin >> n;
	int c = 0;
	int** a = new int *[n*n];
	for (int i = 0; i < n; i++)
	{
		a[i] = new int[n];
	}
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < n; j++)
		{
			if (a[i][j] < 0) c++;
		}
	}
	cout << c;
}
