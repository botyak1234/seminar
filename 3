#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
using namespace std;



int main()
{
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	double x, y;
	cin >> x >> y;
	if (x >= -1 && x <= 1 && y >= -3 && y <= 3 && (x * x + y * y >= 4))
	{
		if (x == -1 || x == 1 || y == 3 || y == -3 || (x * x + y * y == 4)) cout << "граница";
		else cout << "внутри";
	}
	else cout << " не попала ";
}
